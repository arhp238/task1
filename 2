#include <iostream>
#include <cmath>

const double PI = 3.14159265358979323846;

int main() {
    double L;
    std::cout << "введите длину: ";
    std::cin >> L;

    double R = L / (2 * PI);
    double S = PI * pow(R, 2);
    std::cout << "S круг = " << S << " квад" << std::endl;

    return 0;
}
